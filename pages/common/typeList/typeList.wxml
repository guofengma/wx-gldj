<template name="typeList">
  <view class='type'>
    <block>
      <view wx:for="{{typeLists}}" wx:key="index">
        <text data-index="{{item.id}}" data-name="{{item.name}}" catchtap='chooseType' class="{{active==item.id?'active':''}}">{{item.name}}</text>
      </view>
    </block>
  </view>
</template>