.footer{
  width:100%;
  height: 94rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9999;
  background-color: #fff;
  display: flex;
  border-top:1px solid #f2f2f2;
}
.footer view,.footer navigator{
  width:33.33%;
  text-align: center;
  font-size:0;
}
.footer view view,.footer navigator view{
  width: 100%;
  text-align: center;
  color:#323232;
  font-size:30rpx;
}
.footer navigator image{
  width:50rpx;
  height: 50rpx;
  margin-top:6rpx;
}
.plus{
  display: flex;
  align-items: center;
  justify-content: center;
}