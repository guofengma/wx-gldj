/* pages/active/activityPreview/userLists/userLists.wxss */
.group-lists{
  width: 100%;
  padding: 0 25rpx;
  box-sizing: border-box;
}
.group-lists image{
  width:68rpx;
  height: 68rpx;
  margin-right:25rpx;
  
}
.select{
  position: absolute;
  right: 0;
  margin: 0 !important;
}
.group-item{
  height: 120rpx;
  position: relative;
  display: flex;
  border-bottom: 1px solid #f0f0f0;
  align-items: center;
  color:#646464;
  font-size: 34rpx;
}
.btn{
  width: 600rpx;
  height: 90rpx;
  margin: 0 auto;
  line-height: 90rpx;
  text-align: center;
  background-color:#c6f5fd;
  border-radius: 40rpx;
  font-weight: bold;
  margin-top:115rpx;
  color:#323232;
}