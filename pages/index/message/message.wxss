/* pages/index/message/message.wxss */
@import '../../common/signList/signList.wxss';
@import '../../common/tabBar/tabBar.wxss';
.stop-scroll{
  overflow-y: hidden;
}
.my-sign{
  width:100%;
  height: 100%;
  position: relative;
}
.fixed{
  width:100%;
  height: 100%;
  position:fixed;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 99;
}
.code-image{
  width:500rpx;
  height: 500rpx;
  position: absolute;
  top:50%;
  left: 50%;
  margin-top:-200rpx;
  margin-left: -250rpx;
  z-index: 100;
}