/* pages/index/message/message.wxss */
@import '../../common/signList/signList.wxss';
@import '../../common/tabBar/tabBar.wxss';
.my-sign{
  width:100%;
  height: 100%;
  position: relative;
}
.no-data{
  text-align: center;
}
.no-data image{
  margin-top: 200rpx;
  width:247rpx;
  height:219rpx;
}
.no-data view{
  padding-top: 30rpx;
  font-size:36rpx;
  color:#cecece;
}
.fixed{
  width:100%;
  height: 100%;
  position:fixed;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 99;
}
.code-image{
  width:500rpx;
  height: 500rpx;
  position: absolute;
  top:50%;
  left: 50%;
  margin-top:-200rpx;
  margin-left: -250rpx;
  z-index: 100;
}