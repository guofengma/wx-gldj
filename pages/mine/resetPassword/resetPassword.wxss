/* pages/mine/resetPassword/resetPassword.wxss */
.reset,form{
  width: 100%;
  height: 100%;
  padding: 50rpx 0;
  box-sizing: border-box;
  background-color: #f0f0f0;
}
form input{
  width: 650rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin: 0 auto;
  margin-bottom: 20rpx;
  height: 80rpx;
  background-color: #fff;
  border-radius: 6px;
}
.btn{
  width: 600rpx;
  height: 90rpx;
  margin: 0 auto;
  line-height: 90rpx;
  text-align: center;
  background-color:#c6f5fd;
  border-radius: 40rpx;
  font-weight: bold;
  margin-top:115rpx;
  margin-bottom:50rpx;
  color:#323232;
}